<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_mrp_bom_form_view_inherit" model="ir.ui.view">
            <field name="name">mrp.bom.view</field>
            <field name="model">mrp.bom</field>
            <field name="inherit_id" ref="mrp.mrp_bom_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook/page/field/tree/field[@name='product_qty']" position="after">
                    <field name="recipe_qty"/>
                </xpath>
                <xpath expr="//notebook/page/field/tree/field[@name='product_qty']" position="attributes">
                    <attribute name="string">Actual Qty to Consume</attribute>
                </xpath>
            </field>
        </record>
    </data>
</odoo>