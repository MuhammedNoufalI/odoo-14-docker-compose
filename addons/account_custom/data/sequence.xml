<odoo>
    <data >

        <!-- Sequences for hr.resignation -->
        <record id="seq_vendor" model="ir.sequence">
            <field name="name">Vendor Seq</field>
            <field name="code">vendor.seq</field>
            <field name="prefix">V/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_customer" model="ir.sequence">
            <field name="name">Customer Seq</field>
            <field name="code">customer.seq</field>
            <field name="prefix">C/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
